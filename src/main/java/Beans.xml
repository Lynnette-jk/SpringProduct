<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <!-- Definition for javaCollection -->
   <bean id = "javaCollection" class = "com.june.JavaCollection">
      
      <!-- results in a setAddressList(java.util.List) call -->
      <property name = "addressList">
         <list>
            <value>Uganda</value>
            <value>Sudan</value>
            <value>KENYA</value>
            <value>KENYA</value>
         </list>
      </property>

      <!-- results in a setAddressSet(java.util.Set) call -->
      <property name = "addressSet">
         <set>
            <value>Uganda</value>
            <value>Sudan</value>
            <value>KENYA</value>
            <value>KENYA</value>
         </set>
      </property>

      <!-- results in a setAddressMap(java.util.Map) call -->
      <property name = "addressMap">
         <map>
            <entry key = "1" value = "Uganda"/>
            <entry key = "2" value = "Sudan"/>
            <entry key = "3" value = "KENYA"/>
            <entry key = "4" value = "KENYA"/>
         </map>
      </property>
      
      <!-- results in a setAddressProp(java.util.Properties) call -->
      <property name = "addressProp">
         <props>
            <prop key = "one">Uganda</prop>
            <prop key = "one">Uganda</prop>
            <prop key = "two">Sudan</prop>
            <prop key = "three">KENYA</prop>
            <prop key = "four">KENYA</prop>
         </props>
      </property>
   </bean>

</beans>